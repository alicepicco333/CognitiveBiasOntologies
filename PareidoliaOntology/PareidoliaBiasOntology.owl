        <rdfs:label>Facial Shape</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://github.com/corrado877/CognitiveBiasOntologies/blob/main/PareidoliaOntology/PareidoliaBiasOntology.owl#FacialShapeIllusion -->

    <owl:NamedIndividual rdf:about="https://github.com/corrado877/CognitiveBiasOntologies/blob/main/PareidoliaOntology/PareidoliaBiasOntology.owl#FacialShapeIllusion">
        <rdf:type rdf:resource="https://dbpedia.org/page/Illusion"/>
