    <!-- https://dbpedia.org/page/Illusion -->

    <owl:Class rdf:about="https://dbpedia.org/page/Illusion">
        <owl:disjointWith rdf:resource="https://github.com/corrado877/CognitiveBiasOntologies/blob/main/CognitiveBiasOntologies#Variables"/>
        <owl:disjointWith rdf:resource="https://w3id.org/framester/data/framestercore/Activity"/>
        <owl:disjointWith rdf:resource="https://w3id.org/framester/data/framestercore/BecomingAware"/>
